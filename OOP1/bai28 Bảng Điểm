package OOPbai28;

/**
 *
 * @author QUANGTUNG
 */
public class HocSinh {
    private String ma, name;
    private double toan, van, anh, li, hoa, sinh, su, dia, cd, cn;

    public HocSinh(int ma, String name, double toan, double van, double anh, double li, double hoa, double sinh, double su, double dia, double cd, double cn) {
        this.ma = "HS" + String.format("%02d", ma);
        this.name = name;
        this.toan = toan;
        this.van = van;
        this.anh = anh;
        this.li = li;
        this.hoa = hoa;
        this.sinh = sinh;
        this.su = su;
        this.dia = dia;
        this.cd = cd;
        this.cn = cn;
    }
    
    public double getDiemTB(){
        return (this.toan + this.van + this.anh + this.li + this.hoa + this.sinh + this.su + this.dia + this.cd + this.cn) / 10;
    }
    
    public String getHocLuc(){
        if(this.getDiemTB() >= 9) return "XUAT SAC";
        else if(this.getDiemTB() >= 8) return "GIOI";
        else if(this.getDiemTB() >= 7) return "KHA";
        else if(this.getDiemTB() >= 5) return "TB";
        else 
            return "YEU";
    }
    
    public String getMa(){
        return this.ma;
    }
    
    public String toString(){
        return this.ma + " " + this.name + " " + String.format("%.1f", this.getDiemTB()) + " " + this.getHocLuc();
    }
}
/*
 * Click nbfs://nbhost/SystemFileSystem/Templates/Licenses/license-default.txt to change this license
 * Click nbfs://nbhost/SystemFileSystem/Templates/Classes/Class.java to edit this template
