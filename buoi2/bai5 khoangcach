import java.util.Scanner;
public class bai1 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int x1 = sc.nextInt();
        int y1 = sc.nextInt();
        int x2 = sc.nextInt();
        int y2 = sc.nextInt();
        System.out.printf("%.2f", Math.sqrt((double)Math.pow(x1 - x2 , 2) + (double)Math.pow(y1 - y2 , 2)));
    }
}
