import java.util.Scanner;
public class bai1 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        System.out.println(a + b);
        System.out.println(a - b);
        System.out.println((long)a * b);
        if(b != 0)
        System.out.printf("%.4f", (double)a / b);
        else
        System.out.println("INVALID");
    }
}
